{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ESNext"],
    "rootDir": ".",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
  },
  "include": ["src/apiHelper/clients/**/*.ts", "src/tests/**/*.ts", "src/apiHelper/utils/**/*.ts", "src/apiHelper/models/**/*.ts", "src/apiHelper/setupClient.ts"],
  "exclude": ["dist", "node_modules"]
}